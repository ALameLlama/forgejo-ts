{
  "name": "@llamaduck/forgejo-ts",
  "version": "11.0.10",
  "description": "Auto-generated TypeScript client for Forgejo API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "node scripts/generate.mjs",
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "check-versions": "node scripts/check-versions.mjs",
    "test": "echo \"No tests yet\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ALameLlama/forgejo-ts.git"
  },
  "keywords": [
    "forgejo",
    "gitea",
    "api",
    "client",
    "typescript"
  ],
  "author": "Nicholas Ciechanowski <nicholas@ciech.anow.ski>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ALameLlama/forgejo-ts/issues"
  },
  "homepage": "https://github.com/ALameLlama/forgejo-ts#readme",
  "dependencies": {
    "axios": "^1.7.0"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "@hey-api/openapi-ts": "^0.92.3",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=20"
  }
}
